{
  "trailingComma": "none",
  "arrowParens": "avoid",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "singleAttributePerLine": true,
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "@/components/(.*)$",
    "@/ui/(.*)$",
    "@/layout/(.*)$",
    "@/lib/(.*)$",
    "@/providers/(.*)$",
    "@/services/(.*)$",
    "@/constants/(.*)$",
    "@/hooks/(.*)$",
    "@/schemas/(.*)$",
    "@/types",
    "@/assets/(.*)$",
    "@/utils/(.*)$",
    "@/api/(.*)$",
    "^../(.*)$",
    "^./(.*)$",
    "(?=./styles.module.css)"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
